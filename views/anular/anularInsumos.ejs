<div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="REtirar Insumos"></h1>
</div>

<form action="/addInsumo" method="POST">
    <div class="row form-group mb-4">
        <div class="col">
            <label for="Unidades_Retiradas">Anular Retiro</label>
            <input type="text" class="form-control" id="Unidades_Retiradas" name="Unidades_Retiradas"
                placeholder="Unidades_Retiradas" required>
        </div>
        <div class="col">
            <label for="producto">Seleccionar Producto</label>
            <select class="form-control" id="producto" name="producto" required>
                <!-- Aquí se mostrarán las opciones de productos -->
                <% productos.forEach(producto => { %>
                    <option value="<%= producto._id %>"><%= producto.nombre %></option>
                <% }); %>
            </select>
        </div>
    </div>

    <div class="form-group mb-4">
        <button type="submit" class="btn btn-primary">Añadir Venta</button>
    </div>
</form>